{
  "name": "setup-kind",
  "version": "0.5.0",
  "private": true,
  "description": "GitHub Action to setup KinD (Kubernetes in Docker)",
  "main": "lib/main.js",
  "scripts": {
    "build": "npm run build:main && npm run build:post",
    "build:main": "ncc build -o dist/main src/main.ts",
    "build:post": "ncc build -o dist/post src/post.ts",
    "lint": "eslint . --ext .ts,.json",
    "lint:fix": "npm run lint --fix",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "test": "mocha --require ts-node/register --recursive ./test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/engineerd/setup-kind.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Engineerd",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^0.6.1",
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/glob": "^0.2.0",
    "@actions/tool-cache": "^1.7.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.33.1",
    "chai": "^4.3.4",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.4",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
